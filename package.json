        "postcss-import": "^12.0.1",
        "@tailwindcss/ui": "^0.5.0",
        "tailwindcss": "^1.6.2",
        "vue": "^2.6.11",
        "vue-json-pretty": "^1.6.7",
        "vue-router": "^3.0.1",
        "vue-template-compiler": "^2.5.21"
    }
